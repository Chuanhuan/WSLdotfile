! Restore Ctrl+Shift+(c|v)
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false

! URxvt*background: [90]#000000
! URxvt*depth: 32
! URxvt*foreground: #FFFFFF

! Xresources file

URxvt*inheritPixmap: true
URxvt*transparent: true
! URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*shading: 33

URxvt*font: xft:FantasqueSansMono-Regular:pixelsize=18:antialias=true
URxvt*boldFont: xft:FantasqueSansMono-Bold:pixelsize=18:antialias=true
URxvt*italicFont: xft:FantasqueSansMono-Italic:pixelsize=18:antialias=true
URxvt*letterSpace: -1

!TABS
URxvt.perl-ext-common : default,tabbed
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    3
URxvt.tabbed.tab-bg:    0
